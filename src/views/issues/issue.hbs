<div id="main">
    {{#if viewData}}
        <h1 class="pageHeader">{{viewData.title}}</h1>
        <img src="{{viewData.avatar}}"/>
        <p>{{viewData.description}}</p>
        <p>{{viewData.iid}}</p>
        <p>{{viewData.status}}</p>

        <a href="/issues/issue/{{viewData.iid}}/close">Close issue</a>
        <br>
        <a href="/issues/issue/{{viewData.iid}}/reopen">Reopen issue</a>
    {{/if}}

</div>